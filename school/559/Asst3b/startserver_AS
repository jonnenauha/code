#
#	Start Script
#

echo 'Starting AS server'

export CLASSPATH=~/559
cd ~/559/Asst3b

javac EcoServer.java
rmic -v1.2 Asst3b/EcoRemoteChannel\$EcoChannelReceiver

cd ..

java -Djava.security.policy=util/rmi.policy Asst3b/EcoServer AS EU

rm -f Asst3b/*.class
cd ~/559/Asst3b

echo 'Ending AS server'
